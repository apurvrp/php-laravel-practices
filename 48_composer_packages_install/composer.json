{
  "name": "apurv-prajapati/composer-autoloading",
  "authors": [
    {
      "name": "Apurv Prajapati",
      "email": "apurv.prajapati27@gmail.com"
    }
  ],
  "require": {
    "illuminate/collections": "^11.29"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "Http\\": "Http/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.5"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
